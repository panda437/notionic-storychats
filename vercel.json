{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/wechat-pay.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/cover.jpg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {"source": "/blog/", "destination": "https://notionic-storychats.vercel.app/"},
    {"source": "/blog/:match*", "destination": "https://notionic-storychats.vercel.app/:match*"},
    {"source": "/", "destination": "https://book-manager-eta.vercel.app/"},
    {"source": "/:match*", "destination": "https://book-manager-eta.vercel.app/:match*"},
    {
      "source": "/blog/notes/:pathname",
      "destination": "/api/htmlrewrite?pathname=:pathname"
    },
    {
      "source": "/blog/notes/:pathname/b/:slug*",
      "destination": "/api/htmlrewrite?pathname=:pathname&slug=/b/:slug*"
    },
    {
      "source": "/blog/notes/:pathname/x/:slug*",
      "destination": "/api/htmlrewrite?pathname=:pathname&slug=/x/:slug*"
    },
    {
      "source": "/blog/api/:slug*",
      "destination": "https://www.craft.do/api/:slug*"
    },
    {
      "source": "/blog/share/static/js/:slug*",
      "destination": "/api/jsrewrite?url=https://www.craft.do/share/static/js/:slug*"
    },
    {
      "source": "/blog/share/static/css/:slug*",
      "destination": "https://www.craft.do/share/static/css/:slug*"
    },
    {
      "source": "/blog/share/static/fonts/:slug*",
      "destination": "https://www.craft.do/share/static/fonts/:slug*"
    },
    {
      "source": "/blog/share/static/media/:slug*",
      "destination": "https://www.craft.do/share/static/media/:slug*"
    },
    {
      "source": "/blog/share/static/craft.webmanifest",
      "destination": "https://www.craft.do/share/static/craft.webmanifest"
    },
    {
      "source": "/blog/assets/js/analytics2.js",
      "destination": "https://www.craft.do/404"
    }
  ]
}
